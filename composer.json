{
  "name": "kalimeromk/facebook-post",
  "description": "This package allow to create, update, delete and get posts from facebook page in laravel application",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Zoran \"Shefot\" Bogoevski",
      "email": "zbogoevski3@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "Kalimeromk\\FacebookPost\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kalimeromk\\FacebookPost\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^10.1.0",
    "phpunit/phpunit": "^11.5.6",
    "rector/rector": "2.0.9"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kalimeromk\\FacebookPost\\FacebookPostServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
